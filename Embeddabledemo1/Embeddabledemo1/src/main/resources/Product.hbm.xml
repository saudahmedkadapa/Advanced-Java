<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "hibernate-mapping-3.0.dtd" >


  <hibernate-mapping>
     <class name="com.demo.beans.Product" table="product12">
        <id name="pid">
           <generator class="assigned"></generator>
        
        </id>
        <property name="pname" type="string" column="pname" not-null="true"></property>
        <property name="qty" type="int"></property>
        <property name="price" type="double"></property>
     </class>
     </hibernate-mapping>
